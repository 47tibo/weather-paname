{
  "expo": {
    "privacy": "public",
    "name": "Météo Paname",
    "slug": "weather-paname",
    "version": "1.0.0",
    "description": "Weather in Paris within one week",
    "githubUrl": "https://github.com/47tibo/weather-paname",
    "orientation": "portrait",
    "primaryColor": "#56C5F9",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#56C5F9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "icon": "./assets/images/icon.png"
    },
    "android": {
      "icon": "./assets/images/icon.png",
      "adaptiveIcon": {
        "backgroundColor": "#56C5F9",
        "foregroundImage": "./assets/images/adaptative-icon-foreground.png",
        "backgroundImage": "./assets/images/adaptative-icon-background.png"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
